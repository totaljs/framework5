                    <xsd:enumeration value="WS_PLUGIN:0005" />
                    <xsd:enumeration value="WS_PLUGIN:0006" />
                    <xsd:enumeration value="WS_PLUGIN:0007" />
                    <xsd:enumeration value="WS_PLUGIN:0008" />
                    <xsd:enumeration value="WS_PLUGIN:0009" />
