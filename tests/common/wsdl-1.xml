            </xsd:simpleType>
            <xsd:simpleType name="errorCode">
                <xsd:restriction base="xsd:string">
                    <xsd:enumeration value="WS_PLUGIN:0001" />
                    <xsd:enumeration value="WS_PLUGIN:0002" />
